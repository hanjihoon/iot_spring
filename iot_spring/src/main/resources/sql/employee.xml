<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="em">
  
  	<select id="selectEmployeeList" resultType="em">
  		SELECT emNo,emName,emId,dpNo,emAd,emEmail,emPhone FROM em_INFO;
  	</select>
  	
  	<select id="selectEmployee" resultType="em" parameterType="int">
  		SELECT emNo,emName,emId,dpNo,emAd,emEmail,emPhone,emSal,emPwd,emReg FROM EM_INFO where emNo=#{emNo};
  	</select>
  	
 
  	
  </mapper>
<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  <mapper namespace="user">
  
  	<select id="selectUserList" resultType="u">
  		SELECT uiNo,uiName,uiAge,uiId,address,ciNo,uiRegdate FROM USER_INFO;
  	</select>
  	
  	<select id="selectUser" resultType="u" parameterType="int">
  		SELECT userNo,userName,userAge,userId,userAddress,ciNo FROM USER_INFO where uiNo=#{uiNo};
  	</select>
  	
 
  	
  </mapper>